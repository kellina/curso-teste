{
    "extends": "airbnb-base",
    "env": {
        "jest": true,
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "eqeqeq": "warn",
        "indent": ["error", 4],
        "arrow-body-style":"off"
    }
}
